eles=document.getElementsByClassName("oajrlxb2 g5ia77u1 qu0x051f esr5mh6w e9989ue4 r7d6kgcz rq0escxv nhd2j8a9 pq6dq46d p7hjln8o kvgmc6g5 cxmmr5t8 oygrvhab hcukyx3x jb3vyjys rz4wbd8a qt6c0cv9 a8nywdso i1ao9s8h esuyzwwr f1sip0of lzcic4wl l9j0dhe7 abiwlrkh p8dawk7l cbu4d94t taijpn5t k4urcfbm");
n=prompt("Number of cancellations:");
for(x=1;x<n;x++){
if (n<=eles.length){
e=eles.length-x;
if (eles[e].getAttribute('aria-label').search("Cancel")!=-1){
eles[(eles.length - 1)].click();}
else{break;}
}}
